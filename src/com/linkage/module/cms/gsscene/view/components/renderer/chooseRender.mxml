<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009"
				xmlns:s="library://ns.adobe.com/flex/spark"
				xmlns:mx="library://ns.adobe.com/flex/mx"
				autoDrawBackground="true">
	<s:HGroup width="100%"
			  height="55"
			  paddingLeft="17"
			  verticalAlign="middle">
		<s:Group width="50">
			<mx:Image source="@Embed('../../../img/areaquickconfig/rain.png')"/>
			<s:Label fontSize="14"
					 left="6"
					 fontWeight="bold"
					 text="{data.flag}"/>
		</s:Group>
		<s:VGroup width="100%"
				  height="100%"
				  gap="3"
				  verticalAlign="middle"
				  horizontalAlign="left">
			<s:Label width="100%"
					 text="{data.mo_name}"/>
			<s:Label width="100%"
					 text="{data.address}"/>
		</s:VGroup>
	</s:HGroup>
</s:ItemRenderer>
