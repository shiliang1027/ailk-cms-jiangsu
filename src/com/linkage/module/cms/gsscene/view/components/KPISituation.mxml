            private function kpi1Over(e:MouseEvent):void
            {
                flag501 = true;
                //									log.info("flag=" + flag501 + "isNew=" + user.isNew);
                //									log.info(e.currentTarget is KpiSmallElement);
                var obj2:Object = new Object;
                obj2.sceneId = user.sceneId;
                obj2.level = user.level;
                obj2.labelId = 501;
