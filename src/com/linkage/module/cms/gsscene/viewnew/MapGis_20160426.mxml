							case "2"://（全省地图）区域定位
							{
								break;
							}
						}
						break;
					}
					case "2"://LTE场景
					{
						switch(user.level)
						{
							case "1"://（全省地图）全省图层
							case "2"://（全省地图）区域LTE图层+定位
							case "3"://（地市地图）地市图层
