			private function addLTEAreaLayerHandler(city:String):void{
				if(this.mapwork.map.getGisLayer("LTEAREA")){
					mapwork.map.removeGisLayer(this.mapwork.map.getGisLayer("LTEAREA"));//删除图层
				}
