					}
				}else{
					if(viewLayerCfg.moFaultCb.selected){
						showMoinfoParams.warnLevelFlag = "2";
					}else{
						scenePM.mapViewDM.pointQuitGisLayer.clear();
						return;
					}
				}
				//增加地市场景信息。
				user.getImportParamsForRemoteObject(showMoinfoParams);
				log.info(showMoinfoParams);
