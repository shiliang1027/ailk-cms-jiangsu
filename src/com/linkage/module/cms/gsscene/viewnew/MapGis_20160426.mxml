							}
							case "2"://（全省地图）全省图层+定位
							{
								if(user.sceneId=="1001"){
									addLayerHandler(_city_id);
								}
								break;
							}
						}
						break;
					}
				}
				
				// loadNodeBType参数：
				//1-->查询撒点，只需要查询框选点内的退服基站；
				//2-->查询基站，及其告警信息；
