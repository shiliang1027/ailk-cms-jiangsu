			protected function doShowMoInfo():void{
				log.warn("[doShowMoInfo]");
				switch(user.logicalType){
					case "1":
					{
					//日常节假日场景
						switch(user.level)
						{
							case "1"://（全省地图）全省图层
							case "3"://（地市地图）地市图层
							case "4"://（全省地图）全省图层+定位
							{
								addLayerHandler(_city_id);
								break;
							}
						}
						break;
				}
					case "2"://LTE场景
					{
						switch(user.level)
						{
							case "1"://（全省地图）全省图层
							case "3"://（地市地图）地市图层
							case "4"://（全省地图）全省图层+定位
							{
								addLayerHandler(_city_id);
								break;
							}
							case "2"://（全省地图）区域LTE图层+定位
							{
								addLTEAreaLayerHandler(_city_id);
								break;
							}
						}
						break;
				}
					case "3":
					{
						//重大活动场景
						switch(user.level)
						{
							case "1"://（全省地图）全省图层
							case "3"://（地市地图）地市图层
							case "4"://（全省地图）全省图层+定位
							{
								addLayerHandler(_city_id);
								break;
							}
						}
						break;
					}
					case "4":
					{
						switch(user.level)
						{
							case "1"://（全省地图）全省图层
							case "3"://（地市地图）地市图层
							case "4"://（全省地图）全省图层+定位
							{
								addLayerHandler(_city_id);
