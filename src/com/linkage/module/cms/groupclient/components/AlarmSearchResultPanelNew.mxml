		private function dealPoint(point:Point,height:Number,mHeight:Number):Point
		{
			var p:Point;
			if(mHeight>height)
			{
				if(mHeight-point.y>height)
				{
					p= point;
				}
				else if(point.y>height)
				{
					p = new Point(point.x,point.y-height);	
				}
				else 
				{
					p = new Point(point.x,mHeight-height);	
				}
			}
			return p;
		}
		
		
