<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009"
				xmlns:s="library://ns.adobe.com/flex/spark"
				xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100%"
				color="0x000000" fontSize="12" fontFamily="微软雅黑" fontStyle="normal" fontWeight="normal"
				x="{data.x}" y="{data.y - 30}" autoDrawBackground="false" >
	
	<s:SkinnableContainer backgroundColor="0xDBDBDB"
						  skinClass="com.linkage.module.cms.roamtopic.view.TipSkinForTuli">
		<s:layout>
			<s:VerticalLayout gap="0" horizontalAlign="center"/>
		</s:layout>
		<s:SkinnableContainer>
			<s:layout>
				<s:HorizontalLayout paddingLeft="50" paddingRight="50" paddingTop="7" paddingBottom="7" horizontalAlign="center"/>
			</s:layout>
			<s:Label text="{data.user_num}"/>
			<s:Label text="/"/>
			<s:Label text="{data.manchu}"/>
		</s:SkinnableContainer>
	</s:SkinnableContainer>
</s:ItemRenderer>
