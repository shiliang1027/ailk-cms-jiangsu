<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009"
				xmlns:s="library://ns.adobe.com/flex/spark"
				xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100%"
				x="{data.x}" y="{data.y-30}" autoDrawBackground="false"
				creationComplete="itemrenderer1_creationCompleteHandler(event)">
	<fx:Script>
		<![CDATA[
			import com.ailk.common.system.logging.ILogger;
			import com.ailk.common.system.logging.Log;
			
			import mx.events.FlexEvent;
			private var log:ILogger=Log.getLoggerByClass(TuLiRenderer);
			protected function itemrenderer1_creationCompleteHandler(event:FlexEvent):void
			{
				// TODO Auto-generated method stub
//				log.info("data");
//				log.info(data);
			}
			
		]]>
	</fx:Script>
	
	<s:SkinnableContainer backgroundColor="0xDBDBDB"
						  skinClass="com.linkage.module.cms.roamtopic.view.TipSkinForTuli">
		<s:layout>
			<s:VerticalLayout gap="0" horizontalAlign="center"/>
		</s:layout>
		<s:HGroup gap="0" color="0x000000" fontSize="12" fontFamily="微软雅黑" fontStyle="normal" fontWeight="normal"> 
			<s:BorderContainer minHeight="0" minWidth="0" borderVisible="false" cornerRadius="5" backgroundAlpha="0.8" backgroundColor="0xDBDBDB">
				<s:layout>
					<s:HorizontalLayout paddingLeft="20" paddingRight="20" paddingTop="5" paddingBottom="2" horizontalAlign="center"/>
				</s:layout>
				<s:Label text="{data.log_nums}"/>
			</s:BorderContainer>
			<s:BorderContainer minHeight="0" minWidth="0" borderVisible="false" cornerRadius="5" backgroundAlpha="0.8" backgroundColor="0xB9B9B9">
				<s:layout>
					<s:HorizontalLayout paddingLeft="20" paddingRight="20" paddingTop="5" paddingBottom="2" horizontalAlign="center"/>
				</s:layout>
				<s:Label text="{data.huanbi}"/>
			</s:BorderContainer>
		</s:HGroup>
		<s:SkinnableContainer>
			<s:layout>
				<s:HorizontalLayout paddingLeft="5" paddingRight="5" paddingTop="5" paddingBottom="3" horizontalAlign="center"/>
			</s:layout>
			<s:Label color="0x000000" text="{data.user_num}" fontSize="12" fontFamily="微软雅黑" fontStyle="normal" fontWeight="normal"/>
		</s:SkinnableContainer>
	</s:SkinnableContainer>
	
	
</s:ItemRenderer>
