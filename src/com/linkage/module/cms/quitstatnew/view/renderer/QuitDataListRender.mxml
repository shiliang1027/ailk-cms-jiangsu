<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009"
				xmlns:s="library://ns.adobe.com/flex/spark"
				xmlns:mx="library://ns.adobe.com/flex/mx"
				width="100%" autoDrawBackground="false"
				xmlns:parsley="http://www.spicefactory.org/parsley">
	<fx:Declarations>
		<parsley:Configure/>
	</fx:Declarations>

	<s:BorderContainer width="100%" minHeight="0">
		<s:layout>
			<s:HorizontalLayout/>
		</s:layout>
		<s:DataGroup width="100%" dataProvider="{data.kpiList}" itemRenderer="com.linkage.module.cms.quitstatnew.view.renderer.QuitDataItemRender">
			<s:layout>
				<s:VerticalLayout gap="1" />
			</s:layout>
		</s:DataGroup>
	</s:BorderContainer>
	

	
</s:ItemRenderer>
