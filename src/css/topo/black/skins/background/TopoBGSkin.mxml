<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
			 xmlns:mx="library://ns.adobe.com/flex/halo"
			 xmlns:s="library://ns.adobe.com/flex/spark"
			 alpha.disabled="0.5">
	<fx:Metadata>
		[HostComponent("spark.components.SkinnableContainer")]
	</fx:Metadata>
	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect left="0"
			right="0"
			top="0"
			bottom="0">
		<s:stroke>
			<s:SolidColorStroke color="0"
								alpha="0.50"
								weight="3"/>
		</s:stroke>
	</s:Rect>
	<s:Rect id="background"
			left="1"
			top="1"
			right="1"
			bottom="1"
			alpha="1">
		<s:fill>
			<s:BitmapFill source="@Embed(source='css/topo/black/assets/background/mainBg2.jpg')"/>
		</s:fill>
	</s:Rect>
	<s:Group id="contentGroup"
			 left="1"
			 right="1"
			 top="1"
			 bottom="1">
		<s:layout>
			<s:VerticalLayout/>
		</s:layout>
	</s:Group>
</s:SparkSkin>
