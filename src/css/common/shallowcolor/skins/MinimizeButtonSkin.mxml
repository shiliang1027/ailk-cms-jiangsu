<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:d="http://ns.adobe.com/fxg/2008/dt" xmlns:mx="library://ns.adobe.com/flex/mx" minWidth="16" minHeight="16">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect id="fill" left="1" right="1" top="1" bottom="1" radiusX="2">
		<s:fill>
			<s:SolidColor color="0xffffff" alpha="0" alpha.over="0.5" alpha.down="0.8"/>
		</s:fill>
	</s:Rect>
	<mx:Image source="@Embed('../assets/minimizeButton.png')" horizontalCenter="0" verticalCenter="0"/>

</s:Skin>
